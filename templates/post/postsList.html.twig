{% extends '/layout/base.html.twig' %}

{% block content %}

{#    {% form_theme form 'forms/postsListTheme.html.twig' %}#}
    {{ statusCode }}
    {{ header }}
    {% for post in posts %}
        <div class="col-md-8 blog-main">
            <div class="blog-post">
                <h2 class="blog-post-title">{{ post.title }}</h2>
                <p>Post id = {{ post.id }}</p>
                <p>User id = {{ post.userId }}</p>
                <p class="background-gray">{{ post.body }}</p>
            </div>
        </div>

    {% endfor %}
{% endblock %}